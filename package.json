{
	"name": "@jonahsnider/benchmark",
	"version": "0.0.0-development",
	"bugs": {
		"url": "https://github.com/jonahsnider/benchmark/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonahsnider/benchmark.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Jonah Snider",
		"email": "jonah@jonahsnider.com",
		"url": "https://jonahsnider.com"
	},
	"sideEffects": false,
	"type": "module",
	"exports": "./tsc_output/src/index.js",
	"types": "./tsc_output/index.d.ts",
	"files": [
		"tsc_output",
		"!tsc_output/{test,examples}"
	],
	"scripts": {
		"build": "tsc",
		"docs": "typedoc",
		"lint": "xo",
		"release": "semantic-release",
		"style": "prettier --check .",
		"test": "ava",
		"test:coverage": "c8 --reporter lcov --reporter text-summary --include 'src/**/*.ts' --exclude 'src/index.ts' --all ava",
		"validate-api": "api-extractor run",
		"validate-api:local": "api-extractor run --local --verbose"
	},
	"prettier": "@jonahsnider/prettier-config",
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"files": [
			"test/**/*.test.ts"
		],
		"nodeArguments": [
			"--loader=ts-node/esm"
		],
		"nonSemVerExperiments": {
			"configurableModuleFormat": true
		}
	},
	"dependencies": {
		"@jonahsnider/util": "9.0.0",
		"tslib": "2.3.1"
	},
	"devDependencies": {
		"@jonahsnider/prettier-config": "1.1.0",
		"@jonahsnider/xo-config": "5.0.4",
		"@microsoft/api-extractor": "7.19.5",
		"@tsconfig/node16": "1.0.2",
		"@types/node": "16.0.3",
		"ava": "3.15.0",
		"c8": "7.11.0",
		"eslint-plugin-prettier": "4.0.0",
		"jest-mock": "27.5.1",
		"prettier": "2.5.1",
		"semantic-release": "18.0.1",
		"ts-node": "10.6.0",
		"typedoc": "0.22.13",
		"typescript": "4.6.3",
		"xo": "0.48.0"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "yarn@3.2.0"
}
